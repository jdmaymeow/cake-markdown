{
    "name": "jdmaymeow/cake-markdown",
    "description": "CakeMarkdown plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0",
        "erusev/parsedown": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakeMarkdown\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeMarkdown\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
